{
    "name": "nhwin/assets-management",
    "description": "Laravel assets management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyễn Huy Thắng",
            "email": "nhthang304@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "laravel/framework": "^5.6|^6|^7|^8|^9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nhwin\\AssetsManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nhwin\\AssetsManagement\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nhwin\\AssetsManagement\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Nhwin\\AssetsManagement\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
